#include <bits/stdc++.h>
using namespace std;

int main(){
int t,n;
string s;
cin>>t;
while(t--){
    cin>>n>>s;
    int i=0;
        if(s[i] == '2' &&s[i+1] == '0'&&s[i+2] == '2'&&s[i+3] == '0'){
            cout<<"Yes"<<endl;
        }
         else if(s[i] == '2' &&s[i+1] == '0'&&s[i+2] == '2'&&s[n-1] == '0'){
            cout<<"Yes"<<endl;
        }
        else if(s[i] == '2' &&s[i+1] == '0'&&s[n-2] == '2'&&s[n-1] == '0'){
            cout<<"Yes"<<endl;
        }
         else if(s[i] == '2' &&s[n-3] == '0'&&s[n-2] == '2'&&s[n-1] == '0'){
            cout<<"Yes"<<endl;
        }
         else if(s[i] != '2' &&s[n-4] == '2'&&s[n-3] == '0'&&s[n-2] == '2' &&s[n-1]=='0'){
            cout<<"Yes"<<endl;
        }
    else{
        cout<<"No"<<endl;
    }
}
return 0;
}
