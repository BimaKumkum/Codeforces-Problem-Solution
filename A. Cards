#include<bits/stdc++.h>
using namespace std;
int main(){
int n;
int zero=0,one=0;
string s;
cin>>n;
cin>>s;
for(int i=0;i<n;i++){
    if(s[i] == 'z'){
       zero++;
    }
    else if(s[i] == 'n'){
        one++;
    }
}
while(one--){
    cout<<'1';
    cout<<" ";
}
while(zero--){
    cout<<'0';
    cout<<" ";
}

}
