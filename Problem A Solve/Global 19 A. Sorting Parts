#include<bits/stdc++.h>
using namespace std;

int main()
{
    long long int t,n,m;
    cin>>t;
    while(t--)
    {
        cin>>n;
        long long int flag = -1,cats=1;
        for(int i=0;i<n;i++)
        {
           cin>>m;
           if(m>=flag)
           {
              flag = m;
           }
           else
           {
               cats = 0;
           }
        }
        if(!cats)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
}
