#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,test_case,x;
    cin>>test_case;
    while(test_case--)
    {
        cin>>n;
        int odd=0,even=0;
        for(int i=0;i<2*n;i++)
        {
            cin>>x;
            if(x % 2 ==0)
                even++;
            else
                odd++;
        }
        if(even == odd)
        {
            cout<<"Yes"<<endl;
        }
        else
        {
            cout<<"No"<<endl;
        }

    }

    return 0;
}

