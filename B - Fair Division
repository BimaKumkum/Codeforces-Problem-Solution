#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,n;
    cin>>t;
    while(t--)
    {
        int w1=0,w2=0,weight;
        cin>>n;
        for(int i=0;i<n;i++)
        {
            cin>>weight;
            if(weight ==1)
            w1++;
            else
                w2++;
        }
        if(w1%2==0 && w2%2==0 ||w1%2==0&&w1>1)
        {
            cout<<"Yes"<<endl;
        }
        else{
             cout<<"No"<<endl;
        }
    }
    return 0;
}
